<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <com.google.android.gms.maps.MapView
        android:id="@+id/map_first_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="2"
        map:cameraTargetLat="31.3"
        map:cameraTargetLng="35.217018"
        map:cameraZoom="7.5" />

    <LinearLayout
        android:id="@+id/linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/textview_border"
        android:orientation="horizontal"
        android:elevation="2dp">

        <AutoCompleteTextView
            android:id="@+id/map_first_input_search_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:layout_weight="1"
            android:hint=" Type your choose"
            android:lines="1" />

<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/map_first_img_search"
            android:layout_width="43dp"
            android:layout_height="43dp"
            android:layout_marginBottom="2dp"
            android:layout_marginRight="2dp"
            android:layout_marginTop="2dp"
            android:clickable="true"
            android:src="@mipmap/ic_search" />
</LinearLayout>
    </LinearLayout>


</RelativeLayout><!--<com.google.android.gms.maps.MapView--><!--android:id="@+id/map_first_fragment"--><!--android:layout_width="match_parent"--><!--android:layout_height="match_parent"--><!--android:layout_weight="2"--><!--map:cameraTargetLat="31.3"--><!--map:cameraTargetLng="35.217018"--><!--map:cameraZoom="7" />-->

